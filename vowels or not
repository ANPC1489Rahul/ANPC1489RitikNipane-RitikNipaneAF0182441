package src;

public class VowelConsonant {

	public static void main(String[] args) {
		
		char rt = 'i';
		
		if(rt == 'a' || rt == 'e' || rt == 'i' || rt == 'o' || rt == 'u');
		  System.out.println(rt + " is vowel");
		else
		  System.out.println(rt + " is consonant");
	

	}

}


output:
i is vowel
